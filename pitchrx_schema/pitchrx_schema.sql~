  `break_angle` float DEFAULT NULL,
  `break_length` float DEFAULT NULL,
  `pitch_type` varchar(16) DEFAULT NULL,
  `type_confidence` float DEFAULT NULL,
  `zone` int(11) DEFAULT NULL,
  `nasty` int(11) DEFAULT NULL,
  `spin_dir` int(11) DEFAULT NULL,
  `spin_rate` int(11) DEFAULT NULL,
  `cc` varchar(255) DEFAULT NULL,
  `mt` varchar(255) DEFAULT NULL,
  `url` varchar(255) NOT NULL,
  `inning_side` varchar(16) DEFAULT NULL,
  `inning` int(11) DEFAULT NULL,
  `next_` varchar(32) DEFAULT NULL,
  `num` int(11) NOT NULL,
  `on_1b` int(11) DEFAULT NULL,
  `on_2b` int(11) DEFAULT NULL,
  `on_3b` int(11) DEFAULT NULL,
  `count` varchar(16) DEFAULT NULL,
  `gameday_link` varchar(255) DEFAULT NULL


