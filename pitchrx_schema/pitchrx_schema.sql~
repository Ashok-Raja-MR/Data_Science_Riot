
CREATE TABLE action
  `b` double DEFAULT NULL,
  `s` int(11) DEFAULT NULL,
  `o` int(11) DEFAULT NULL,
  `des` varchar(255) DEFAULT NULL,
  `des_es` varchar(255) DEFAULT NULL,
  `event` varchar(128) DEFAULT NULL,
  `tfs` varchar(50) DEFAULT NULL,
  `tfs_zulu` varchar(50) DEFAULT NULL,
  `player` int(11) DEFAULT NULL,
  `pitch` int(11) DEFAULT NULL,
  `url` varchar(255) DEFAULT NULL,
  `inning_side` varchar(32) DEFAULT NULL,
  `inning` int(11) DEFAULT NULL,
  `next_` varchar(32) DEFAULT NULL,
  `num` varchar(32) DEFAULT NULL,
  `score` varchar(32) DEFAULT NULL,
  `home_team_runs` varchar(32) DEFAULT NULL,
  `away_team_runs` varchar(32) DEFAULT NULL,
  `event2` varchar(128) DEFAULT NULL,
  `gameday_link` varchar(255) DEFAULT NULL



